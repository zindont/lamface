    height: auto;
    width: 100%;
}

/*--------------------------------------------------------------
## CSS Animation
--------------------------------------------------------------*/

@keyframes fadein {
    from { opacity: 0; }
    to   { opacity: 1; }
}

/* Firefox < 16 */
@-moz-keyframes fadein {
    from { opacity: 0; }
    to   { opacity: 1; }
}

/* Safari, Chrome and Opera > 12.1 */
@-webkit-keyframes fadein {
    from { opacity: 0; }
    to   { opacity: 1; }
}

/* Internet Explorer */
@-ms-keyframes fadein {
    from { opacity: 0; }
    to   { opacity: 1; }
}

/* Opera < 12.1 */
@-o-keyframes fadein {
    from { opacity: 0; }
    to   { opacity: 1; }
}

@-webkit-keyframes rotation {
	from {
			-webkit-transform: rotate(0deg);
	}
	to {
			-webkit-transform: rotate(359deg);
	}
}

@keyframes type{ 
	from { width: 0; } 
  } 
  
@keyframes type2{
	0%{width: 0;}
	50%{width: 0;}
	100%{ width: 100; } 
} 

@keyframes blink{
	to{opacity: .0;}
}

