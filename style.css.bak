/*--------------------------------------------------------------
## Layout
--------------------------------------------------------------*/

#menu-left, #content {
	float: left;
}

#menu-left {
	left: 0px;
	border-right: none;
	width: 20%;
	position: fixed;
	top: 0px;
	bottom: 0px;
	overflow: hidden;
    padding: 20px 20px 0px 40px;

	animation: 0.5s cubic-bezier(.25,1.1,.5,1) 0.1s showLeftMenu;
    animation-fill-mode: none;
	animation-fill-mode: both;
}

.scroller {
	padding-right: 20px;
    height: 90%;
    overflow-y: scroll;
}


#content {
	width: 60%;
	margin-top: 30px;
	margin-left: 20%;
	-webkit-transition: all 0.2s ease;
	transition: all 0.2s ease;
	position: relative;
	border-top: none;
}

#menu-right {
	width: 20%;
	float: right;
	right: 0px;
	display: table-cell;
    -webkit-transition: all 0.2s ease;
    transition: all 0.2s ease;
    border-left: none;
    border-radius: 0px 0px 0px 50px;
    background-color: #fff;
}

#menu-right .widget-area {
    background-image: none;
    -webkit-box-shadow: 0px 0px 24px 0px rgba(151,160,204,0.12);
    box-shadow: 0px 0px 24px 0px rgba(151,160,204,0.12);
    padding: 30px 30px 50px;
    position: relative;
    color: #3c5b68;
}

footer {
	clear: both;
    float: none;
    margin-left: 20%;
    padding: 20px 0px;
    font-size: 13px;
    font-weight: 500;
}

