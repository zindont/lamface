/*--------------------------------------------------------------
## Mansory
--------------------------------------------------------------*/

/* ---- grid ---- */

.mansory-grid {
  max-width: 1200px;
  width: 100%;
}

/* clearfix */
.mansory-grid:after {
  content: '';
  display: block;
  clear: both;
}

/* ---- grid-item ---- */

.grid-sizer,
.mansory-grid .item-featured {
  width: 31.5%;
}
